# BUG: Comparison operators return f64 instead of integer.
# `3 < 5` produces 1.0 (double). Passing to printf with %d
# should work if comparisons returned integer, but will fail
# because the result is f64 and %d expects integer.
#
# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x 'cmp=1' %t
# RUN: ! grep -q "Error (Line:" %t

def main() -> i32:
    # 3 < 5 should give integer 1, but gives f64 1.0
    # printf %d will reject the float type
    printf("cmp=%d\n", 3 < 5)
    return 0

main()
