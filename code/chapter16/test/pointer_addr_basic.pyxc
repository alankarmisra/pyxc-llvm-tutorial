extern def printd(x: f64) -> f64
extern def putchard(x: f64) -> f64

type int = i32

def main() -> i32:
    x: int = 10
    p: ptr[int] = addr(x)
    p[0] = p[0] + 1
    printd(x)
    putchard(10)
    return 0

main()
