# RUN: %pyxc < %s > %t 2>&1
# RUN: grep -Fq "!!" %t
# RUN: grep -Fq "!!******************" %t
# RUN: test "$(grep -c 'Evaluated to 0.000000' %t | tr -d '[:space:]')" = "2"

extern def putchard(x)

def printpattern(n):
    if n < 3:
        for i in range(1, n+1, 1):
            putchard(33) # '!'
    else:
        for i in range(1, n+1, 1):
            if i < 3:
                putchard(33) # '!'
            else:
                putchard(42) # '*'

printpattern(2)
printpattern(20)
