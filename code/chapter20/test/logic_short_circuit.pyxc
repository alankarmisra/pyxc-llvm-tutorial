# RUN: %pyxc -i %s > %t 2>&1
# RUN: test $(grep -o "77.000000" %t | wc -l) -eq 2
# RUN: ! grep -q "Error (Line:" %t

extern def printd(x)

def mark():
    printd(77)
    return 5

def main():
    printd(0 and mark())  # skip rhs
    printd(1 and mark())  # eval rhs
    printd(1 or mark())   # skip rhs
    printd(0 or mark())   # eval rhs
    return 0

main()
