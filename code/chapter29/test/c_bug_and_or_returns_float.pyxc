# BUG: `and`/`or` operators return f64 instead of integer.
# `1 and 1` produces 1.0 (double) via UIToFP.
# Passing to printf %d will fail due to type mismatch.
#
# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x 'and=1 or=1' %t
# RUN: ! grep -q "Error (Line:" %t

def main() -> i32:
    printf("and=%d or=%d\n", 1 and 1, 0 or 1)
    return 0

main()
