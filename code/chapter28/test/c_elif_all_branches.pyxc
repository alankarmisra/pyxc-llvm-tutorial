# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x 'one' %t
# RUN: grep -x 'two' %t
# RUN: grep -x 'other' %t
# RUN: ! grep -q "Error (Line:" %t

def check(x: i32) -> i32:
    if x == 1:
        printf("one\n")
    elif x == 2:
        printf("two\n")
    else:
        printf("other\n")
    return 0

def main() -> i32:
    check(1)
    check(2)
    check(99)
    return 0

main()
