# BUG: Modulo uses CreateSRem (signed) for all integer types.
# For unsigned types this should be CreateURem.
# u8 200 % 7: unsigned = 200 % 7 = 4.
# signed (200 as i8 = -56): -56 % 7 = 0 (or impl-defined).
#
# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x '4' %t
# RUN: ! grep -q "Error (Line:" %t

def main() -> i32:
    x: u8 = 200
    r: i32 = x % 7
    print(r)
    return 0

main()
