# BUG: EBNF says identifiers can start with underscore:
#   letter = "A".."Z" | "a".."z" | "_"
# But lexer line 457 uses isalpha() which does NOT match '_'.
# So `_foo` will lex as unknown token '_' then identifier 'foo'.
# This test should compile cleanly if underscores work.
#
# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x '99' %t
# RUN: ! grep -q "Error (Line:" %t

def main() -> i32:
    _count: i32 = 99
    print(_count)
    return 0

main()
