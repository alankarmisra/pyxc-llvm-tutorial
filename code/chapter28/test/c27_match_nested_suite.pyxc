# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x 'sum=6' %t
# RUN: ! grep -q "Error (Line:" %t

def main() -> i32:
    x: i32 = 3
    total: i32 = 0
    match x:
        case 3:
            for i in range(1, 4, 1):
                total = total + i
        case _:
            total = -1
    printf("sum=%d\n", total)
    return 0

main()
