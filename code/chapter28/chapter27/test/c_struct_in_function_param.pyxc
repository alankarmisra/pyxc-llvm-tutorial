# Test passing struct by value to a function (if supported).
# Structs are value types, so this tests that struct copy works.
# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x '10 20' %t
# RUN: ! grep -q "Error (Line:" %t

struct Point:
    x: i32
    y: i32

def print_point(p: Point) -> void:
    print(p.x, p.y)
    return

def main() -> i32:
    p: Point
    p.x = 10
    p.y = 20
    print_point(p)
    return 0

main()
