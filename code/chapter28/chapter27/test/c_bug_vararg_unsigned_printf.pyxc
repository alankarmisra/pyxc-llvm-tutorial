# BUG: Vararg integer promotion uses CreateSExt for all narrow integers.
# For u8, this sign-extends: u8 200 -> i32 -56 -> printf prints -56.
# Expected: printf should print 200.
#
# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x 'val=200' %t
# RUN: ! grep -q "Error (Line:" %t

def main() -> i32:
    x: u8 = 200
    printf("val=%d\n", x)
    return 0

main()
