# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x 'positive-even' %t
# RUN: ! grep -q "Error (Line:" %t

def main() -> i32:
    x: i32 = 4
    if x > 0:
        if x % 2 == 0:
            printf("positive-even\n")
        else:
            printf("positive-odd\n")
    else:
        printf("non-positive\n")
    return 0

main()
