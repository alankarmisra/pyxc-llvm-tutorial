# Test short-circuit evaluation of `or`.
# If first operand is true, second should NOT be evaluated.
# RUN: %pyxc -i %s > %t 2>&1
# RUN: grep -x 'reached' %t
# RUN: ! grep -q 'side-effect' %t
# RUN: ! grep -q "Error (Line:" %t

def side() -> i32:
    printf("side-effect\n")
    return 1

def main() -> i32:
    # 1 or side() should short-circuit: side() never called
    x: i32 = 1
    if x or side():
        printf("reached\n")
    return 0

main()
