# RUN: printf 'A 7\n' | %pyxc -i %s > %t 2>&1
# RUN: grep -x 'letter upper' %t
# RUN: grep -x 'digit=7' %t
# RUN: ! grep -q "Error (Line:" %t

def main() -> i32:
    ch: i8 = 0
    n: i32 = 0
    scanf("%c %d", addr(ch), addr(n))
    if isalpha(ch):
        if ch == toupper(ch):
            printf("letter upper\n")
    if isdigit(n + 48):
        printf("digit=%d\n", n)
    return 0

main()
